{
  "name": "touchstream",
  "version": "0.1.0",
  "private": true,
  "author": "Judit Sánchez Garrido <judit@jodiehag.com> (https://github.com/jodiehag)",
  "contributors": [
    "Judit Sánchez Garrido <judit@jodiehag.com> (https://github.com/jodiehag)"
  ],
  "scripts": {
    "develop": "next dev",
    "deploy": "next build && next export",
    "lint:fix": "next lint --fix",
    "compile:i18n": "formatjs compile-folder --ast --format simple public/static/locales static/compiled-locales",
    "extract:i18n": "formatjs extract '{pages,components,sections}/**/*.{js,ts,tsx}' --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file public/static/locales/en.json",
    "i18n": "yarn run compile:i18n",
    "build": "next build",
    "heroku-prebuild": "node ./bin/auth-npm.js",
    "start": "next start",
    "lint:package": "sort-package-json",
    "prepare": "husky install",
    "lint": "next lint"
  },
  "lint-staged": {
    "package.json": [
      "yarn lint:package"
    ],
    "src/**/**/*.{js,jsx}": []
  },
  "dependencies": {
    "@jellybrains/marvin": "1.9.0",
    "@reduxjs/toolkit": "^1.9.1",
    "@styled-system/css": "^5.1.5",
    "@styled-system/prop-types": "^5.1.5",
    "babel-plugin-formatjs": "^10.3.35",
    "babel-plugin-styled-components": "^2.0.7",
    "cloudinary-core": "^2.13.0",
    "eslint-plugin-prettier": "^4.2.1",
    "intl": "^1.2.5",
    "moment": "^2.29.4",
    "next": "^13.1.4",
    "next-redux-wrapper": "^8.1.0",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.2.5",
    "react-redux": "^8.0.5",
    "sharp": "^0.31.3",
    "styled-components": "^5.3.6",
    "styled-system": "^5.1.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "eslint": "8.23.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^13.1.4",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "sort-package-json": "^2.2.0"
  },
  "peerDependencies": {
    "@styled-system/css": "^5.1.5",
    "next": "13.1.4",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "18.2.0",
    "react-select": "^4.1.0",
    "styled-components": "^5.3.3",
    "styled-system": "^5.1.5"
  }
}
